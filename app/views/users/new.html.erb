<div class="main">
	<div class="container">
		<h1 class="join">JOIN <span>G</span>ENERAL <span>A</span>SSEMBLY</h1>
			<%= form_for(@user) do |f| %>
				<div class="form-group">
					<%=f.label :name %>
					<%=f.text_field :name, class: 'form-control' %>
				</div>
				<div class="form-group">
					<%=f.label :email %>
					<%=f.email_field :email, class: 'form-control' %>
				</div>
				<div class="form-group">
					<%=f.label :password %>
					<%=f.password_field :password, class: 'form-control' %>
				</div>
			<button>Create account</button>
			<% end %>
	</div>
</div>


